{
  "name": "axe-charts",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ./scripts/build-pages.js",
    "build-watch": "nodemon ./scripts/build-pages.js --watch client --watch scripts --ext html,css,js",
    "local": "http-server ./dist -a localhost -p 3000",
    "local-watch": "npm run build && concurrently \"npm run build-watch\" \"npm run local\""
  },
  "dependencies": {
    "fs-extra": "^11.1.0",
    "mustache": "^4.2.0",
    "puppeteer": "^19.7.2",
    "sqlite3": "^5.1.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "http-server": "^14.1.1",
    "nodemon": "^3.0.1"
  }
}
